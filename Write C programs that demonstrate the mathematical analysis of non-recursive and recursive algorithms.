#include <stdio.h>
int sumNonRecursive(int n) {
    int sum = 0;
    for (int i = 1; i <= n; i++) {
        sum += i;
    }
    return sum;
}

int main() {
    int n;

    printf("Enter a positive integer n: ");
    scanf("%d", &n);
    int result = sumNonRecursive(n);
    
    printf("Sum of first %d natural numbers: %d\n", n, result);

    return 0;
}
